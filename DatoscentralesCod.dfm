  object scriptCarga: TFDScript
    SQLScripts = <
      item
        Name = 'Usuarios'
        SQL.Strings = (
          'CREATE TABLE IF NOT EXISTS USUARIOS ('
          '    USUARIO         TEXT (63) PRIMARY KEY'
          '                              UNIQUE'
          '                              NOT NULL,'
          '    CLAVESECRETA    TEXT (63) NOT NULL,'
          '    NOMBRE          TEXT (64) NOT NULL,'
          '    APELLIDOPATERNO TEXT (64) NOT NULL,'
          '    APELLIDOMATERNO TEXT (64),'
          '    ESTADOACTUAL    TEXT (16) DEFAULT Pendiente'
          '                              NOT NULL'
          ');')
      end
      item
        Name = 'Sitios'
        SQL.Strings = (
          'CREATE TABLE IF NOT EXISTS SITIOS ('
          '    SITIO          INTEGER    PRIMARY KEY,'
          '    NOMBREDELSITIO TEXT (255) NOT NULL,'
          '    LATITUD        TEXT (32)  NOT NULL,'
          '    LONGITUD       TEXT (32)  NOT NULL'
          ');')
      end
      item
        Name = 'Proyectos'
        SQL.Strings = (
          'CREATE TABLE IF NOT EXISTS PROYECTOS ('
          '    PROYECTO              INTEGER UNIQUE'
          '                                  PRIMARY KEY,'
          '    DESCRIPCION           TEXT    NOT NULL,'
          
            '    SITIO                 INTEGER REFERENCES SITIOS (SITIO) ON U' +
            'PDATE CASCADE,'
          '    TODOSPUEDENTOMARFOTOS INTEGER DEFAULT (0) '
          ');')
      end
      item
        Name = 'Conceptos'
        SQL.Strings = (
          'CREATE TABLE IF NOT EXISTS CONCEPTOSDELPROYECTO ('
          '    CONCEPTODELPROYECTO  INTEGER    NOT NULL'
          '                                    PRIMARY KEY,'
          '    DESCRIPCIONDELAETAPA TEXT (256) NOT NULL,'
          '    PROYECTO             INTEGER    NOT NULL'
          
            '                                    REFERENCES PROYECTOS (PROYEC' +
            'TO) ON DELETE NO ACTION'
          
            '                                                                ' +
            '    ON UPDATE CASCADE'
          ');')
      end
      item
        Name = 'Fotos'
        SQL.Strings = (
          'CREATE TABLE IF NOT EXISTS FOTOS ('
          '    FOTO                INTEGER   PRIMARY KEY AUTOINCREMENT,'
          '    IMAGEN              BLOB      NOT NULL,'
          '    LATITUD             TEXT (32) NOT NULL,'
          '    LONGITUD            TEXT (32) NOT NULL,'
          '    USUARIOENVIADOR     TEXT (63) NOT NULL'
          
            '                                  REFERENCES USUARIOS (USUARIO) ' +
            'ON DELETE RESTRICT'
          
            '                                                                ' +
            'ON UPDATE CASCADE,'
          '    FECHADETOMA         TEXT      NOT NULL'
          '                                  DEFAULT (CURRENT_TIMESTAMP),'
          '    FECHADEENVIO        TEXT,'
          
            '    CONCEPTODELPROYECTO INTEGER   REFERENCES CONCEPTOSDELPROYECT' +
            'O (CONCEPTODELPROYECTO) ON UPDATE CASCADE,'
          
            '    PROYECTO            INTEGER   REFERENCES PROYECTOS (PROYECTO' +
            ') ON UPDATE CASCADE'
          '                                  NOT NULL,'
          '    STATUS              TEXT (64) NOT NULL'
          '                                  DEFAULT ('#39'Pendiente'#39'),'
          '    COMENTARIOS         BLOB'
          ');')
      end
      item
        Name = 'ASIGNACIONES'
        SQL.Strings = (
          'CREATE TABLE IF NOT EXISTS ASIGNACIONES ('
          '    ASIGNACION INTEGER   PRIMARY KEY'
          '                         UNIQUE,'
          
            '    PROYECTO   INTEGER   REFERENCES PROYECTOS (PROYECTO) ON DELE' +
            'TE RESTRICT'
          
            '                                                         ON UPDA' +
            'TE CASCADE,'
          
            '    USUARIO    TEXT (63) REFERENCES USUARIOS (USUARIO) ON DELETE' +
            ' RESTRICT'
          
            '                                                       ON UPDATE' +
            ' CASCADE'
          ');')
      end
      item
        Name = 'Vistas'
        SQL.Strings = (
          'CREATE VIEW IF NOT EXISTS PROYECTOSDELUSUARIO ('
          '    PROYECTO,'
          '    DESCRIPCION,'
          '    SITIO,'
          '    TODOSPUEDENTOMARFOTOS,'
          '    USUARIO'
          ')'
          'AS'
          '    SELECT PROY.PROYECTO,'
          '           PROY.DESCRIPCION,'
          '           PROY.SITIO,'
          '           PROY.TODOSPUEDENTOMARFOTOS,'
          '           ASI.USUARIO'
          '      FROM PROYECTOS PROY'
          '           LEFT OUTER JOIN'
          '           ASIGNACIONES ASI ON PROY.PROYECTO = ASI.PROYECTOS;'
          'CREATE VIEW IF NOT EXISTS SITIOSDELUSUARIO ('
          '    SITIO,'
          '    LATITUD,'
          '    LONGITUD,'
          '    NOMBREDELSITIO,'
          '    USUARIO'
          ')'
          'AS'
          '    SELECT SIT.SITIO,'
          '           SIT.LATITUD,'
          '           SIT.LONGITUD,'
          '           SIT.NOMBREDELSITIO,'
          '           PROY.USUARIO'
          '      FROM SITIOS SIT'
          '           JOIN'
          '           PROYECTOSDELUSUARIO PROY ON (SIT.SITIO = PROY.SITIO);')
      end>
